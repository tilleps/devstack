{
  "name": "devstack",
  "version": "0.1.8",
  "description": "Collection of scripts and functions",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.js",
      "import": "./lib/index.mjs",
      "default": "./lib/index.mjs"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "require": "./dist/*.js",
      "import": "./lib/*.mjs",
      "default": "./lib/*.mjs"
    }
  },
  "types": "./index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write lib/ .prettierrc.json index.d.ts",
    "lint": "eslint -c ./eslint.config.mjs ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tilleps/devstack.git"
  },
  "author": "Eugene Song <tilleps@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tilleps/devstack/issues"
  },
  "homepage": "https://github.com/tilleps/devstack#readme",
  "peerDependencies": {
    "ejs": ">= 3.0.0",
    "express": ">= 4.0.0",
    "knex": ">= 3.0.0",
    "mysql2": ">=3.11.0",
    "passport": ">=0.7.0",
    "pino": ">= 8.0.0",
    "validate.js": "^0.13.1"
  },
  "peerDependenciesMeta": {
    "ejs": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "knex": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "passport": {
      "optional": true
    },
    "pino": {
      "optional": true
    }
  },
  "dependencies": {
    "express-async-errors": "^3.1.1",
    "http-terminator": "^3.2.0"
  },
  "devDependencies": {
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.11.0",
    "prettier": "^2.8.8",
    "tap": "^21.0.0"
  }
}
